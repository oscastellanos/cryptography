
  mpz_t huge;
  mpz_t big;


  mpz_init_set_str (huge, "1232135435268754612315895456213", 10);
  mpz_init_set_str (huge, "546523154687654621321", 10);
  unsigned long result;
  mpz_gcd_ui(result, huge,big); 

  cout << "The gcd(1232135435268754612315895456213, 654652315487654621321) is: "
  << result << endl;



